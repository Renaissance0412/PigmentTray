<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.DawnStars.pigmenttray">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RUNTIME" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".TestActivity"></activity>
        <activity android:name=".ViewConcreteActivity" />
        <activity android:name=".ViewPhotoActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".TakePhotoActivity" />
        <activity android:name=".ViewResultActivity" />
        <activity android:name=".PhotoBeautyActivity" />
        <activity android:name=".ViewWorksActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlbumActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>